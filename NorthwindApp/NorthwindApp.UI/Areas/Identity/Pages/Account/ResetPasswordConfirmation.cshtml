@page
@model ResetPasswordConfirmationModel
@{
    ViewBag.Title = "Reset password confirmation";
}

<div class="row">
    <div class="col-md-12">
        <h2 class="page-header">@ViewBag.Title</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <p>Your password has been reset. Please <a asp-page="./Login">click here to log in</a>.</p>
    </div>
</div>
