@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
@{
    ViewBag.Title = "Users";
}

<div class="row">
    <div class="col-md-12">
        <h2 class="page-header">Users</h2>
    </div>
</div>
<div class="row items-list">
    <div class="col-md-12">
        @foreach (var user in Model)
        {
            <p>@user.UserName</p>
        }
    </div>
</div>
